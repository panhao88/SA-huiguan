pe: Number,\r\n    },\r\n  },\r\n  components: {},\r\n  data() {\r\n    return {\r\n      currentPage4: 1, //当前页数\r\n      pagesize: 5, //一页多少条\r\n      pagenum: 1, //当前页数\r\n      query: \"\",\r\n      dialogFormVisible: false,\r\n      dialogForm: false,\r\n      id: \"\",\r\n      options: [\r\n        {\r\n          id: 0,\r\n          name: \"否\",\r\n        },\r\n        {\r\n          id: 1,\r\n          name: \"是\",\r\n        },\r\n      ],\r\n      rulegoto: {\r\n        content: \"\",\r\n        termination: \"\",\r\n        lastAt: \"\",\r\n        terminationAt: \"\",\r\n      },\r\n      ruleForm: {\r\n        content: \"\",\r\n        termination: \"\",\r\n        lastAt: \"\",\r\n        terminationAt: \"\",\r\n      },\r\n      pickerOptions1: {\r\n        shortcuts: [\r\n          {\r\n            text: \"今天\",\r\n            onClick(picker) {\r\n              picker.$emit(\"pick\", new Date());\r\n            },\r\n          },\r\n          {\r\n            text: \"昨天\",\r\n            onClick(picker) {\r\n              const date = new Date();\r\n              date.setTime(date.getTime() - 3600 * 1000 * 24);\r\n              picker.$emit(\"pick\", date);\r\n            },\r\n          },\r\n          {\r\n            text: \"一周前\",\r\n            onClick(picker) {\r\n              const date = new Date();\r\n              date.setTime(date.getTime() - 3600 * 1000 * 24 * 7);\r\n              picker.$emit(\"pick\", date);\r\n            },\r\n          },\r\n        ],\r\n      },\r\n      rules: {\r\n        content: [\r\n          {\r\n            required: true,\r\n            message: \"跟单内容不能为空\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n      },\r\n      formLabelWidth: \"110px\",\r\n    };\r\n  },\r\n  methods: {\r\n    ...userActions([\r\n      \"getmerchandis\",\r\n      \"addmerchandis\",\r\n      \"detelemerchandis\",\r\n      \"updatemerchandis\",\r\n    ]),\r\n    //编辑\r\n    handleEdit(index, row) {\r\n      this.dialogForm = true;\r\n      this.id = row.id;\r\n      this.ruleForm.content = row.content;\r\n      this.ruleForm.termination = row.termination;\r\n      this.ruleForm.lastAt = row.lastAt;\r\n      this.ruleForm.terminationAt = row.terminationAt;\r\n    },\r\n    //确认编辑\r\n    affirm(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.dialogForm = false;\r\n          this.updatemerchandis({\r\n            id: this.id,\r\n            cid: this.ids,\r\n            content: this.ruleForm.content,\r\n            termination: this.ruleForm.termination,\r\n            lastAt: this.ruleForm.lastAt,\r\n            terminationAt: this.ruleForm.terminationAt,\r\n            pageNo: this.pagenum,\r\n            pageSize: this.pagesize,\r\n          });\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    //删除\r\n    handleDelete(index, row) {\r\n      this.$confirm(\"此操作将永久删除该文件, 是否继续?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n        .then(() => {\r\n          this.detelemerchandis({\r\n            id: row.id,\r\n            cid: this.ids,\r\n            pageNo: this.pagenum,\r\n            pageSize: this.pagesize,\r\n          });\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"已取消删除\",\r\n          });\r\n        });\r\n    },\r\n    // 添加\r\n    add(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.dialogFormVisible = false;\r\n          this.addmerchandis({\r\n            cid: this.ids,\r\n            content: this.rulegoto.content,\r\n            termination: this.rulegoto.termination,\r\n            lastAt: this.rulegoto.lastAt,\r\n            terminationAt: this.rulegoto.terminationAt,\r\n            pageNo: this.pagenum,\r\n            pageSize: this.pagesize,\r\n          });\r\n          this.rulegoto = {};\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    //分页每页多少条\r\n    handleSizeChange(val) {\r\n      this.pagesize = val;\r\n      this.getmerchandis({\r\n        cid: this.ids,\r\n        pageNo: this.pagenum,\r\n        pageSize: this.pagesize,\r\n      });\r\n    },\r\n    //分页当前是第几页\r\n    handleCurrentChange(val) {\r\n      this.pagenum = val;\r\n      this.getmerchandis({\r\n        cid: this.ids,\r\n        pageNo: this.pagenum,\r\n        pageSize: this.pagesize,\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getmerchandis({\r\n      cid: this.ids,\r\n      pageNo: this.pagenum,\r\n      pageSize: this.pagesize,\r\n    });\r\n  },\r\n  watch: {},\r\n  computed: {\r\n    ...userState([\"merchandislist\", \"totalRowg\"]),\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped lang='scss'>\r\n /deep/.el-tooltip__popper {\r\n    max-width: 100px;\r\n    line-height: 180%;\r\n  }\r\n\r\n</style>"]}]}