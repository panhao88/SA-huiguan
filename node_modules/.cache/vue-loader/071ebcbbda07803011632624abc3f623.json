">\r\n      <el-form-item label=\"昵称\">\r\n        <el-input size=\"mini\" v-model=\"s.nickName\" placeholder=\"昵称\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"用户名\">\r\n        <el-input size=\"mini\" v-model=\"s.userName\" placeholder=\"用户名\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"电话号码\">\r\n        <el-input size=\"mini\" v-model=\"s.phone\" placeholder=\"电话号码\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"用工形式\">\r\n        <el-select size=\"mini\" v-model=\"s.recruit\" placeholder=\"用工形式\">\r\n          <el-option label=\"全职\" value=\"0\"></el-option>\r\n          <el-option label=\"兼职\" value=\"1\"></el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"职员状态\">\r\n        <el-select size=\"mini\" v-model=\"s.state\" placeholder=\"职员状态\">\r\n          <el-option label=\"试用\" value=\"0\"></el-option>\r\n          <el-option label=\"转正\" value=\"1\"></el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"账户状态\">\r\n        <el-select size=\"mini\" v-model=\"s.status\" placeholder=\"账户状态\">\r\n          <el-option label=\"正常\" value=\"1\"></el-option>\r\n          <el-option label=\"锁定\" value=\"-1\"></el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button size=\"mini\" type=\"primary\" @click=\"onSubmit\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"text\" @click=\"moreSeach = true\">更多</el-button>\r\n      </el-form-item>\r\n      <!--  按钮  -->\r\n    </el-form>\r\n    <!-- 高级搜索   -->\r\n    <el-dialog title=\"高级搜索\" :visible.sync=\"moreSeach\" :append-to-body=\"true\">\r\n      <el-form :model=\"m\" :inline=\"true\"  class=\"demo-form-inline\">\r\n\r\n        <el-form-item label=\"昵称\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"m.nickName\" auto-complete=\"off\