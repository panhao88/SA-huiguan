      </el-col>\r\n                <el-col :span=\"8\">\r\n                  <el-form-item label=\"专业:\">{{ s.row.major }}</el-form-item>\r\n                </el-col>\r\n                <el-col :span=\"8\">\r\n                  <el-form-item label=\"毕业时间:\">{{ s.row.graduationAt }}</el-form-item>\r\n                </el-col>\r\n                <el-col :span=\"8\">\r\n                  <el-form-item label=\"毕业学校:\">{{ s.row.graduationSchool }}</el-form-item>\r\n                </el-col>\r\n                <el-col :span=\"8\">\r\n                  <el-form-item label=\"本司入职时间:\">{{ s.row.entryAt }}</el-form-item>\r\n                </el-col>\r\n                <el-col :span=\"8\">\r\n                  <el-form-item label=\"本司工龄:\">{{ s.row.workYear }}</el-form-item>\r\n                </el-col>\r\n                <el-col :span=\"8\">\r\n                  <el-form-item label=\"社会保险缴纳时间:\">{{ s.row.socialInsuranceAt }}</el-form-item>\r\n                </el-col>\r\n                <el-col :span=\"8\">\r\n                  <el-form-item label=\"合同签订时间:\">{{ s.row.contractSigningAt }}</el-form-item>\r\n                </el-col>\r\n                <el-col :span=\"8\">\r\n                  <el-form-item label=\"合同到期时间:\">{{ s.row.contractExpirationAt }}</el-form-item>\r\n                </el-col>\r\n                <el-col :span=\"8\">\r\n                  <el-form-item label=\"续签合同时间:\">{{ s.row.contractRenewalAt }}</el-form-item>\r\n                </el-col>\r\n              </el-row>\r\n            </el-form>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"id\" label=\"编号\" width=\"80\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"nickName\" label=\"昵称\" width=\"100\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"userName\" label=\"用户名\" width=\"120\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"phone\" label=\"电话\" width=\"120\" align=\"center\"></el-ta