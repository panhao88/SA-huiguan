<i class=\"el-icon-edit\"></i>\r\n              </el-button>\r\n            </el-tooltip>\r\n            <el-tooltip content=\"删除账户\" placement=\"top\">\r\n              <el-button\r\n                  size=\"mini\"\r\n                  type=\"danger\"\r\n                  @click=\"handleDelete(scope.$index, scope.row)\"\r\n              >\r\n                <i class=\"el-icon-delete\"></i>\r\n              </el-button>\r\n            </el-tooltip>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!-- 账户新增 -->\r\n      <el-dialog title=\"账户新增\" :visible.sync=\"dialogFormVisible\" width=\"60%\" :modal-append-to-body=\"false\" top=\"5vh\">\r\n        <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\">\r\n          <el-row>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"用户名\" :label-width=\"formLabelWidth\" prop=\"username\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.username\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"密码\" :label-width=\"formLabelWidth\" prop=\"password\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.password\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"昵称\" :label-width=\"formLabelWidth\" prop=\"nickname\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.nickname\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"联系电话\" :label-width=\"formLabelWidth\" prop=\"phone\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.phone\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"组织/校区\" :label-width=\"formLabelWidth\" prop=\"campus\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.campus\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"事业中心\" :label-width=\"formLabelWidth\" prop=\"cause\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.cause\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"职务\" :label-width=\"formLabelWidth\" prop=\"job\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.job\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"职员状态\" :label-width=\"formLabelWidth\" prop=\"state\">\r\n                <el-select v-model=\"ruleForm.state\" size=\"mini\"  placeholder=\"职员状态\">\r\n                  <el-option label=\"试用\" value=\"0\"></el-option>\r\n                  <el-option label=\"转正\" value=\"1\"></el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"用工形式\" :label-width=\"formLabelWidth\" prop=\"recruit\">\r\n                <el-select v-model=\"ruleForm.recruit\" size=\"mini\"  placeholder=\"用工形式\">\r\n                  <el-option label=\"全职\" value=\"0\"></el-option>\r\n                  <el-option label=\"兼职\" value=\"1\"></el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"身份证号码\" :label-width=\"formLabelWidth\" prop=\"idCard\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.idCard\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"性别\" :label-width=\"formLabelWidth\" prop=\"sex\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.sex\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"出生年月\" :label-width=\"formLabelWidth\" prop=\"birth\">\r\n                <el-date-picker\r\n                    size=\"mini\"\r\n                    v-model=\"ruleForm.birth\"\r\n                    align=\"right\"\r\n                    type=\"date\"\r\n                    placeholder=\"选择日期\">\r\n                </el-date-picker>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"户籍地址\" :label-width=\"formLabelWidth\" prop=\"permanentAddr\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.permanentAddr\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"现住地址\" :label-width=\"formLabelWidth\" prop=\"presentAddr\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.presentAddr\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"紧急联系人\" :label-width=\"formLabelWidth\" prop=\"emergencyContact\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.emergencyContact\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"紧急联系人电话\" :label-width=\"formLabelWidth\" prop=\"emergencyContactNum\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.emergencyContactNum\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"民族\" :label-width=\"formLabelWidth\" prop=\"nation\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.nation\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"婚姻状况\" :label-width=\"formLabelWidth\" prop=\"marriage\">\r\n                <el-select v-model=\"ruleForm.marriage\" size=\"mini\"  placeholder=\"婚姻状况\">\r\n                  <el-option label=\"未婚\" value=\"0\"></el-option>\r\n                  <el-option label=\"已婚\" value=\"1\"></el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"学历\" :label-width=\"formLabelWidth\" prop=\"education\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.education\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"专业\" :label-width=\"formLabelWidth\" prop=\"major\">\r\n                <el-input size=\"mini\" v-model=\"ruleForm.major\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"毕业时间\" :label-width=\"formLabelWidth\" prop=\"graduationAt\">\r\n                <el-date-picker\r\n                    size=\"mini\"\r\n                    v-model=\"ruleForm.graduationAt\"\r\n                    align=\"right\"\r\n                    type=\"date\"\r\n                    placeholder=\"选择日期\">\r\n                </el-date-picker>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col span=\"8\">\r\n              <el-form-item label=\"毕业学校\" :label-width=\"formLabelWidth\" prop=\"graduationSchool\">\r\n                <el-input si