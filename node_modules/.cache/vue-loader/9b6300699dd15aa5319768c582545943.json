\r\n      <div>\r\n        <div class=\"c-item\">\r\n          <el-input\r\n            size=\"mini\"\r\n            v-model.trim=\"query\"\r\n            placeholder=\"请输入内容\"\r\n            @keydown.enter.native=\"keydowm\"\r\n          ></el-input>\r\n        </div>\r\n        <div class=\"c-item\" style=\"min-width: 0px\">\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            icon=\"el-icon-search\"\r\n            @click=\"search_se\"\r\n            >查询</el-button\r\n          >\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"dialogFormVisible = true\"\r\n            >添加咨询方式</el-button\r\n          >\r\n        </div>\r\n      </div>\r\n    </el-card>\r\n    <el-card>\r\n      <el-table :data=\"advisory\" style=\"width: 100%\" border align=\"center\">\r\n        <el-table-column\r\n          label=\"ID\"\r\n          width=\"100\"\r\n          prop=\"id\"\r\n          align=\"center\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"name\"\r\n          label=\"名称\"\r\n          align=\"center\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"describe\"\r\n          label=\"描述\"\r\n          align=\"center\"\r\n        ></el-table-column>\r\n        <!-- 操纵 -->\r\n        <el-table-column\r\n          label=\"操作\"\r\n          align=\"center\"\r\n          :show-overflow-tooltip=\"true\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <el-tooltip content=\"编辑\" placement=\"top\">\r\n              <el-button\r\n                size=\"mini\"\r\n                @click=\"handleEdit(scope.$index, scope.row)\"\r\n                type=\"primary\"\r\n              >\r\n                <i class=\"el-icon-edit\"></i>\r\n              </el-button>\r\n            </el-tooltip>\r\n            <el-tooltip content=\"删除\" placement=\"top\">\r\n              <el-button\r\n                size=\"mini\"\r\n                type=\"danger\"\r\n                @click=\"handleDelete(scope.$index, scope.row)\"\r\n              >\r\n                <i class=\"el-icon-delete\"></i>\r\n              </el-button>\r\n            </el-tooltip>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <div style=\"margin-top: 20px\">\r\n        <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pagenum\"\r\n          :page-sizes=\"[5, 10, 15, 20]\"\r\n          :page-size=\"pagesize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"sum\"\r\n        ></el-pagination>\r\n      </div>\r\n      <!-- 添加咨询方式 -->\r\n      <el-dialog\r\n        title=\"添加咨询方式\"\r\n        :visible.sync=\"dialogFormVisible\"\r\n        width=\"30%\"\r\n        :modal-append-to-body=\"false\"\r\n      >\r\n        <el-form :model=\"rulegoto\" :rules=\"rules\" ref=\"rulegoto\">\r\n          <el-form-item label=\"名称\" :label-width=\"formLabelWidth\" prop=\"name\">\r\n            <el-input v-model=\"rulegoto.name\" auto-complete=\"off\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item\r\n            label=\"描述\"\r\n            :label-width=\"formLabelWidth\"\r\n            prop=\"describe\"\r\n          >\r\n            <el-input\r\n              v-model=\"rulegoto.describe\"\r\n              auto-complete=\"off\"\r\n            ></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"add('rulegoto')\">确 定</el-button>\r\n        </div>\r\n      </el-dialog>\r\n      <!-- 编辑来源 -->\r\n      <el-dialog\r\n        title=\"编辑咨询方式\"\r\n        :visible.sync=\"dialogForm\"\r\n        width=\"30%\"\r\n        :modal-append-to-body=\"false\"\r\n      >\r\n        <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\">\r\n          <el-form-item label=\"名称\" :label-width=\"formLabelWidth\" prop=\"name\">\r\n            <el-input v-model=\"ruleForm.name\" auto-complete=\"off\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item\r\n            label=\"描述\"\r\n            :label-width=\"formLabelWidth\"\r\n            prop=\"describe\"\r\n          >\r\n            <el-input\r\n              v-model=\"ruleForm.describe\"\r\n              auto-complete=\"off\"\r\n            ></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogForm = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"affirm('ruleForm')\"\r\n            >确 定</el-button\r\n          >\r\n        </div>\r\n      </el-dialog>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { createNamespacedHelpers } from \"vuex\";\r\nconst userModule = createNamespacedHelpers(\"customer\");\r\nconst { mapState: userState, mapActions: userActions } = userModule;\r\nexport default {\r\n  name: \"\",\r\n  props: {},\r\n  components: {},\r\n  data() {\r\n    return {\r\n      currentPage4: 1, //当前页数\r\n      pagesize: 5, //一页多少条\r\n      pagenum: 1, //当前页数\r\n      query: \"\",\r\n      dialogFormVisible: false,\r\n      dialogForm: false,\r\n      id: \"\",\r\n      rulegoto: {\r\n        name: \"\",\r\n        describe: \"\",\r\n      },\r\n      ruleForm: {\r\n        name: \"\",\r\n        describe: \"\",\r\n      },\r\n      pickerOptions1: {\r\n        shortcuts: [\r\n          {\r\n            text: \"今天\",\r\n            onClick(picker) {\r\n              picker.$emit(\"pick\", new Date());\r\n            },\r\n          },\r\n          {\r\n            text: \"昨天\",\r\n            onClick(picker) {\r\n              const date = new Date();\r\n              date.setTime(date.getTime() - 3600 * 1000 * 24);\r\n              picker.$emit(\"pick\", date);\r\n            },\r\n          },\r\n          {\r\n            text: \"一周前\",\r\n            onClick(picker) {\r\n              const date = new Date();\r\n              date.setTime(date.getTime() - 3600 * 1000 * 24 * 7);\r\n              picker.$emit(\"pick\", date);\r\n            },\r\n          },\r\n        ],\r\n      },\r\n      rules: {\r\n        name: [\r\n          {\r\n            required: true,\r\n            message: \"名称不能为空\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n        describe: [\r\n          {\r\n            required: true,\r\n            message: \"描述不能为空\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n      },\r\n      formLabelWidth: \"70px\",\r\n    };\r\n  },\r\n  methods: {\r\n    ...userActions([\r\n      \"getadvisory\",\r\n      \"addadvisory\",\r\n      \"updateadvisory\",\r\n      \"deleteadvisory\",\r\n    ]),\r\n    //编辑\r\n    handleEdit(index, row) {\r\n      this.id = row.id;\r\n      this.ruleForm.name = row.name;\r\n      this.ruleForm.describe = row.describe;\r\n      this.dialogForm = true;\r\n    },\r\n    //确认编辑\r\n    affirm(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.dialogForm = false;\r\n          this.updateadvisory({\r\n            id: this.id,\r\n            name: this.ruleForm.name,\r\n            describe: this.ruleForm.describe,\r\n            pageNo: this.pagenum,\r\n            pageSize: this.pagesize,\r\n          });\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    //删除\r\n    handleDelete(index, row) {\r\n      this.$confirm(\"此操作将永久删除该文件, 是否继续?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n        .then(() => {\r\n          this.deleteadvisory({\r\n            id: row.id,\r\n            pageNo: this.pagenum,\r\n            pageSize: this.pagesize,\r\n          });\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"已取消删除\",\r\n          });\r\n        });\r\n    },\r\n    // 添加\r\n    add(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.dialogFormVisible = false;\r\n          this.addadvisory({\r\n            name: this.rulegoto.name,\r\n            describe: this.rulegoto.describe,\r\n            pageNo: this.pagenum,\r\n            pageSize: this.pagesize,\r\n          });\r\n          this.rulegoto = {};\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    //分页每页多少条\r\n    handleSizeChange(val) {\r\n      this.pagesize = val;\r\n      this.getadvisory({\r\n        pageNo: this.pagenum,\r\n        pageSize: this.pagesize,\r\n      });\r\n    },\r\n    //分页当前是第几页\r\n    handleCurrentChange(val) {\r\n      this.pagenum = val;\r\n      this.getadvisory({\r\n        pageNo: this.pagenum,\r\n        pageSize: this.pagesize,\r\n      });\r\n    },\r\n    // 搜索\r\n    search_se() {},\r\n    //回车搜索\r\n    keydowm() {},\r\n  },\r\n  mounted() {\r\n    this.getadvisory({\r\n      pageNo: this.pagenum,\r\n      pageSize: this.pagesize,\r\n    });\r\n  },\r\n  watch: {},\r\n  computed: {\r\n    ...userState([\"sum\", \"advisory\"]),\r\n  },\r\n};\r\n</script>\r\n<style scoped lang='scss'>\r\n</style>"]}]}