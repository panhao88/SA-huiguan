    <el-form\n        :model=\"ruleForm\"\n        :rules=\"rules\"\n        ref=\"ruleForm\"\n        label-position=\"left\"\n        label-width=\"0\"\n        class=\"demo-ruleForm\"\n      >\n        <h2 class=\"title\">欢迎来到慧冠后台管理系统</h2>\n        <el-form-item prop=\"username\" style=\"position: relative\">\n          <el-input v-model=\"ruleForm.username\" placeholder=\"账号\"></el-input>\n          <img src=\"../../assets/username.png\" alt=\"\" class=\"img_abs\" />\n        </el-form-item>\n        <el-form-item prop=\"password\" style=\"position: relative\">\n          <el-input\n            v-model=\"ruleForm.password\"\n            placeholder=\"密码\"\n            show-password\n          ></el-input>\n          <img src=\"../../assets/password.png\" alt=\"\" class=\"img_abs\" />\n        </el-form-item>\n        <!-- <el-form-item prop=\"password\" style=\"position: relative\">\n          <el-input\n            placeholder=\"验证码\"\n            style=\"width:60%\"\n          ></el-input>\n          <div>\n            <img src=\"\" alt=\"\">\n          </div>\n        </el-form-item> -->\n        <el-form-item class=\"denglu\">\n          <el-button\n            :loading=\"loading\"\n            type=\"primary\"\n            style=\"width: 100%; margin-bottom: 30px\"\n            @click=\"login('ruleForm')\"\n            >登录</el-button\n          >\n        </el-form-item>\n      </el-form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"\",\n  // 接收父组件传递过来的参数\n  props: {},\n  // 注册组件\n  components: {},\n  // 定义变量\n  data() {\n    return {\n      loading: false,\n      ruleForm: {\n        username: \"sa\",\n        password: \"111111\",\n      },\n      user: {},\n      rules: {\n        username: [\n          {\n            required: true,\n            message: \"用户名不能为空\",\n            trigger: \"blur\",\n          },\n        ],\n        password: [\n          {\n            required: true,\n            message: \"密码不能位空\",\n            trigger: \"blur\",\n          },\n          {\n            min: 6,\n            max: 15,\n            message: \"密码在错误\",\n            trigger: \"blur\",\n          },\n        ],\n      },\n    };\n  },\n  // 事件方法执行\n  methods: {\n    login(formName) {\n      this.loading = true;\n      this.$refs[formName].validate((valid) => {\n        if (valid) {\n           this.$store.dispatch(\"user/login\", {\n            username: this.ruleForm.username,\n            password: this.ruleForm.password,\n          });\n        } else {\n          this.loading = false\n          console.log(\"error submit!!\");\n          return false;\n        }\n      });\n    },\n    // 获取验证码\n\n  },\n  // 页面初始化方法\n  mounted() {},\n  // 监听值变化\n  watch: {},\n  // 计算\n  computed: {},\n};\n</script>\n\n<style scoped lang='scss'>\n.demo-ruleForm {\n  margin-top: -50px;\n  background: white;\n  opacity: 0.8;\n  width: 25vw;\n  padding: 30px 50px 50px 50px;\n  border-radius: 7px;\n}\n.title {\n  margin: 0 auto 30px auto;\n  text-align: center;\n  color: #525151;\n}\n\n.img_abs {\n  position: absolute;\n  left: 6px;\n  top: 13px;\n}\n::v-deep .el-input__inner {\n  padding: 0 30px;\n}\n.lizi {\n  background: linear-gradient(-180deg, #1a1454 0%, #0e81a5 100%);\n  width: 100vw;\n  height: 100vh;\n  // border: 1px solid red;\n  position: relative;\n}\n.login_box {\n  position: absolute;\n  top: 35%;\n  left: 50%;\n  transform: translate(-50%, 0);\n}\n\n@media screen and (max-width: 450px) {\n  // 表单\n  .demo-ruleForm {\n    margin-top: -50px;\n    background: rgba($color: rgb(218, 214, 214), $alpha: 0.3);\n    width: 55vw;\n    padding: 30px 50px 50px 50px;\n    border-radius: 7px;\n  }\n  // 粒子图\n  .lizi {\n    background-repeat: no-repeat;\n    background-size: 100% 100%;\n    height: 100vh;\n    position: relative;\n  }\n}\n</style>\n"]}]}