AR,CAAYP,GAAZ,EAAiB,UAAjB;AATuD;AAAA;;AAAA;AAAA;AAAA;AAWvDM,gBAAAA,OAAO,CAACC,GAAR;;AAXuD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAa9D,KA9CI;AA+CL;AACMS,IAAAA,YAhDD,sCAgDwF;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAxEP,gBAAAA,QAAwE,SAAxEA,QAAwE;AAA1DM,gBAAAA,EAA0D,SAA1DA,EAA0D,EAAtDlB,GAAsD,SAAtDA,GAAsD,EAAlDoB,WAAkD,SAAlDA,WAAkD,EAAtCN,MAAsC,SAAtCA,MAAsC,EAA/BC,SAA+B,SAA/BA,SAA+B,EAApBd,MAAoB,SAApBA,MAAoB,EAAZC,QAAY,SAAZA,QAAY;AAAA;AAAA;AAAA,uBAErEhB,GAAG,CAACiC,YAAJ,CAAiB;AAAED,kBAAAA,EAAE,EAAFA,EAAF;AAAMlB,kBAAAA,GAAG,EAAHA,GAAN;AAAUoB,kBAAAA,WAAW,EAAXA,WAAV;AAAsBN,kBAAAA,MAAM,EAANA,MAAtB;AAA6BC,kBAAAA,SAAS,EAATA;AAA7B,iBAAjB,CAFqE;;AAAA;AAEjFZ,gBAAAA,GAFiF;;AAGrF,oBAAIA,GAAG,CAACC,IAAJ,KAAa,GAAjB,EAAsB;AAClBQ,kBAAAA,QAAQ,CAAC,WAAD,EAAc;AAAEZ,oBAAAA,GAAG,EAAHA,GAAF;AAAMC,oBAAAA,MAAM,EAANA,MAAN;AAAcC,oBAAAA,QAAQ,EAARA;AAAd,mBAAd,CAAR;AACAd,kBAAAA,OAAO,CAAC4B,OAAR,CAAgBb,GAAG,CAACG,GAApB;AACH,iBAHD,MAGM;AACFlB,kBAAAA,OAAO,CAACiB,KAAR,CAAcF,GAAG,CAACG,GAAlB;AACH;;AACDG,gBAAAA,OAAO,CAACC,GAAR,CAAYP,GAAZ,EAAiB,QAAjB;AATqF;AAAA;;AAAA;AAAA;AAAA;AAWrFM,gBAAAA,OAAO,CAACC,GAAR;;AAXqF;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAa5F;AA7DI;AAjBE,CAAf","sourcesContent":["import Vue from 'vue'\r\nimport api from '../../http/api'\r\nimport router from '../../router'\r\nimport { Message } from 'element-ui'\r\n\r\nexport default {\r\n    // 开启命名空间,这个文件就是单独的一个vuex的模块\r\n    namespaced: true,\r\n    state: {\r\n        arrivelist:[] ,//到访列表\r\n        totalRow:0, // //到访总数\r\n    },\r\n    mutations: {\r\n        //到访列表\r\n        setarrive(state,data){\r\n            state.arrivelist = data\r\n        },\r\n        //到访总数\r\n        settotalRow(state,data){\r\n            state.totalRow = data\r\n        }\r\n    },\r\n    actions: {\r\n           //到访信息列表\r\n           async getarrive({ commit }, { cid,pageNo, pageSize }) {\r\n            try {\r\n                let res = await api.getarrive({cid,pageNo, pageSize })\r\n                if (res.code === 200) {\r\n                    commit('setarrive', res.data)\r\n                    commit('settotalRow', res.totalRow)\r\n                }else{\r\n                    Message.error(res.msg)\r\n                    localStorage.setItem(\"showbox\",true)\r\n                }\r\n                console.log(res, \"到访信息列表\")\r\n            } catch (err) {\r\n                console.log(err)\r\n            }\r\n        },\r\n        // 到访信息列表新增\r\n        async addarrive({ dispatch }, { cid,predictAt,campus,reception, pageNo, pageSize }) {\r\n            try {\r\n                let res = await api.addarrive({ cid,predictAt,campus,reception, })\r\n                if (res.code === 200) {\r\n                    dispatch(\"getarrive\", { cid,pageNo, pageSize })\r\n                    Message.success(res.msg)\r\n                }else {\r\n                    Message.error(res.msg)\r\n                }\r\n                console.log(res, \"到访信息列表新增\")\r\n            } catch (err) {\r\n                console.log(err)\r\n            }\r\n        },\r\n        // 到访信息列表删除\r\n        async detelearrive({ dispatch }, { id,cid, pageNo, pageSize }) {\r\n            try {\r\n                let res = await api.detelearrive({ id,cid})\r\n                if (res.code === 200) {\r\n                    dispatch(\"getarrive\", { cid,pageNo, pageSize })\r\n                    Message.success(res.msg)\r\n                }else {\r\n                    Message.error(res.msg)\r\n                }\r\n                console.log(res, \"到访信息列表删除\")\r\n            } catch (err) {\r\n                console.log(err)\r\n            }\r\n        },\r\n        // 到访信息编辑\r\n        async updatearrive({ dispatch }, { id, cid,practicalAt,campus,reception, pageNo, pageSize }) {\r\n            try {\r\n                let res = await api.updatearrive({ id, cid,practicalAt,campus,reception })\r\n                if (res.code === 200) {\r\n                    dispatch(\"getarrive\", { cid,pageNo, pageSize })\r\n                    Message.success(res.msg)\r\n                }else {\r\n                    Message.error(res.msg)\r\n                }\r\n                console.log(res, \"到访信息编辑\")\r\n            } catch (err) {\r\n                console.log(err)\r\n            }\r\n        },\r\n    }\r\n}"]}]}