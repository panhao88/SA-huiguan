AAnDA,EAAmD,EAA/CX,IAA+C,SAA/CA,IAA+C,EAAzCK,QAAyC,SAAzCA,QAAyC,EAA/BC,SAA+B,SAA/BA,SAA+B,EAApBT,MAAoB,SAApBA,MAAoB,EAAZC,QAAY,SAAZA,QAAY;AAAA;AAAA;AAAA,uBAE9Df,GAAG,CAAC6B,YAAJ,CAAiB;AAAED,kBAAAA,EAAE,EAAFA,EAAF;AAAMX,kBAAAA,IAAI,EAAJA,IAAN;AAAYK,kBAAAA,QAAQ,EAARA,QAAZ;AAAsBC,kBAAAA,SAAS,EAATA;AAAtB,iBAAjB,CAF8D;;AAAA;AAE1EP,gBAAAA,GAF0E;;AAG9E,oBAAIA,GAAG,CAACC,IAAJ,KAAa,GAAjB,EAAsB;AAClBI,kBAAAA,QAAQ,CAAC,WAAD,EAAc;AAAEP,oBAAAA,MAAM,EAANA,MAAF;AAAUC,oBAAAA,QAAQ,EAARA;AAAV,mBAAd,CAAR;AACAb,kBAAAA,OAAO,CAACsB,OAAR,CAAgBR,GAAG,CAACS,GAApB;AACH,iBAHD,MAGM,IAAGT,GAAG,CAACC,IAAJ,KAAa,GAAhB,EAAoB;AACtBf,kBAAAA,OAAO,CAACwB,KAAR,CAAcV,GAAG,CAACS,GAAlB;AACH;;AACDP,gBAAAA,OAAO,CAACC,GAAR,CAAYH,GAAZ,EAAiB,MAAjB;AAT8E;AAAA;;AAAA;AAAA;AAAA;AAW9EE,gBAAAA,OAAO,CAACC,GAAR;;AAX8E;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAarF;AA1DI;AAjBE,CAAf,C,CA8EA;AACA;AACA;AACA;AACA;AAEA;AACA;AACA;AACA;AAEA;AACA;AAEA;AACA;AAEA;AACA","sourcesContent":["import Vue from 'vue'\r\nimport api from '../../http/api'\r\nimport router from '../../router'\r\nimport { Message } from 'element-ui'\r\n\r\nexport default {\r\n    // 开启命名空间,这个文件就是单独的一个vuex的模块\r\n    namespaced: true,\r\n    state: {\r\n        campuslistL: [],//校区信息\r\n        totalRow: 0 //校区总数\r\n    },\r\n    mutations: {\r\n        //校区信息\r\n        setcampus(state, data) {\r\n            state.campuslistL = data\r\n        },\r\n        // 校区总数\r\n        settotalRow(state, data) {\r\n            state.totalRow = data\r\n        }\r\n    },\r\n    actions: {\r\n        //校区信息\r\n        async getcampus({ commit }, { pageNo, pageSize }) {\r\n            try {\r\n                let res = await api.getcampus({ pageNo, pageSize })\r\n                if (res.code === 200) {\r\n                    commit('setcampus', res.data)\r\n                    commit('settotalRow', res.totalRow)\r\n                }\r\n                console.log(res, \"校区信息\")\r\n            } catch (err) {\r\n                console.log(err)\r\n            }\r\n        },\r\n        // 校区新增\r\n        async addcampus({ dispatch }, { code, fullName, shortName, pageNo, pageSize }) {\r\n            try {\r\n                let res = await api.addcampus({ code, fullName, shortName })\r\n                if (res.code === 200) {\r\n                    dispatch(\"getcampus\", { pageNo, pageSize })\r\n                    Message.success(res.msg)\r\n                }else if(res.code === 401){\r\n                    Message.error(res.msg)\r\n                }\r\n                console.log(res, \"校区新增\")\r\n            } catch (err) {\r\n                console.log(err)\r\n            }\r\n        },\r\n        // 校区删除\r\n        async delcampus({ dispatch }, { id, pageNo, pageSize }) {\r\n            try {\r\n                let res = await api.delcampus({ id })\r\n                if (res.code === 200) {\r\n                    dispatch(\"getcampus\", { pageNo, pageSize })\r\n                    Message.success(res.msg)\r\n                }else if(res.code === 403){\r\n                    Message.error(res.msg)\r\n                }\r\n                console.log(res, \"校区删除\")\r\n            } catch (err) {\r\n                console.log(err)\r\n            }\r\n        },\r\n        // 校区编辑\r\n        async updataupdate({ dispatch }, { id, code, fullName, shortName, pageNo, pageSize }) {\r\n            try {\r\n                let res = await api.updataupdate({ id, code, fullName, shortName })\r\n                if (res.code === 200) {\r\n                    dispatch(\"getcampus\", { pageNo, pageSize })\r\n                    Message.success(res.msg)\r\n                }else if(res.code === 401){\r\n                    Message.error(res.msg)\r\n                }\r\n                console.log(res, \"校区删除\")\r\n            } catch (err) {\r\n                console.log(err)\r\n            }\r\n        },\r\n    }\r\n}\r\n//模板\r\n// import Vue from 'vue'\r\n// import api from '../../http/api'\r\n// import router from '../../router'\r\n// import { Message } from 'element-ui'\r\n\r\n// export default {\r\n//     // 开启命名空间,这个文件就是单独的一个vuex的模块\r\n//     namespaced: true,\r\n//     state: {\r\n\r\n//     },\r\n//     mutations: {\r\n\r\n//     },\r\n//     actions: {\r\n\r\n//     }\r\n// }"]}]}