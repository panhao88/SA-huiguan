</el-button\r\n        >\r\n      </div>\r\n    </el-card>\r\n    <el-card>\r\n      <el-table\r\n        :data=\"merchandislist\"\r\n        style=\"width: 100%\"\r\n        border\r\n        align=\"center\"\r\n      >\r\n        <el-table-column\r\n          label=\"ID\"\r\n          width=\"60\"\r\n          prop=\"id\"\r\n          align=\"center\"\r\n          fixed=\"left\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"cid\"\r\n          label=\"客户ID\"\r\n          align=\"center\"\r\n          width=\"70\"\r\n          fixed=\"left\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"createAc\"\r\n          label=\"跟单人\"\r\n          width=\"100\"\r\n          align=\"center\"\r\n          fixed=\"left\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"content\"\r\n          label=\"跟单内容\"\r\n          align=\"center\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"createAt\"\r\n          label=\"跟单时间\"\r\n          width=\"160\"\r\n          align=\"center\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"lastAt\"\r\n          label=\"下次跟单时间\"\r\n          width=\"180\"\r\n          align=\"center\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"termination\"\r\n          label=\"终止跟单\"\r\n          align=\"center\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"terminationAt\"\r\n          label=\"终止时间\"\r\n          width=\"160\"\r\n          align=\"center\"\r\n        ></el-table-column>\r\n        <!-- 操纵 -->\r\n        <el-table-column\r\n          label=\"操作\"\r\n          align=\"center\"\r\n          :show-overflow-tooltip=\"true\"\r\n          fixed=\"right\"\r\n          width=\"160\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <el-tooltip content=\"编辑\" placement=\"top\">\r\n              <el-button\r\n                size=\"mini\"\r\n                @click=\"handleEdit(scope.$index, scope.row)\"\r\n                type=\"primary\"\r\n              >\r\n                <i class=\"el-icon-edit\"></i>\r\n              </el-button>\r\n            </el-tooltip>\r\n            <el-tooltip content=\"删除\" placement=\"top\">\r\n              <el-button\r\n                size=\"mini\"\r\n                type=\"danger\"\r\n                @click=\"handleDelete(scope.$index, scope.row)\"\r\n              >\r\n                <i class=\"el-icon-delete\"></i>\r\n              </el-button>\r\n            </el-tooltip>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <div style=\"margin-top: 20px\">\r\n        <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pagenum\"\r\n          :page-sizes=\"[5, 10, 15, 20]\"\r\n          :page-size=\"pagesize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"totalRowg\"\r\n        ></el-pagination>\r\n      </div>\r\n      <!-- 新增跟单 -->\r\n      <el-dialog\r\n        top=\"5vh\"\r\n        title=\"新增跟单\"\r\n        :visible.sync=\"dialogFormVisible\"\r\n        width=\"30%\"\r\n        :append-to-body=\"true\"\r\n      >\r\n        <el-form :model=\"rulegoto\" :rules=\"rules\" ref=\"rulegoto\">\r\n          <el-form-item\r\n            label=\"跟单内容\"\r\n            :label-width=\"formLabelWidth\"\r\n            prop=\"content\"\r\n          >\r\n            <el-input\r\n              type=\"textarea\"\r\n              v-model=\"rulegoto.content\"\r\n            ></el-input>\r\n          </el-form-item>\r\n          <el-form-item\r\n            label=\"跟单终止\"\r\n            :label-width=\"formLabelWidth\"\r\n            prop=\"termination\"\r\n          >\r\n            <el-select v-model=\"rulegoto.termination\" placeholder=\"请选择\">\r\n              <el-option\r\n                v-for=\"item in options\"\r\n                :key=\"item.id\"\r\n                :label=\"item.name\"\r\n                :value=\"item.id\"\r\n              >\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item\r\n            label=\"下次跟单时间\"\r\n            :label-width=\"formLabelWidth\"\r\n            prop=\"lastAt\"\r\n          >\r\n            <el-date-picker\r\n              v-model=\"rulegoto.lastAt\"\r\n              type=\"datetime\"\r\n              placeholder=\"选择日期时间\"\r\n              size=\"mini\"\r\n            >\r\n            </el-date-picker>\r\n          </el-form-item>\r\n          <el-form-item\r\n            label=\"终止时间\"\r\n            :label-width=\"formLabelWidth\"\r\n            prop=\"terminationAt\"\r\n          >\r\n            <el-date-picker\r\n              v-model=\"rulegoto.terminationAt\"\r\n              type=\"datetime\"\r\n              placeholder=\"选择日期时间\"\r\n              size=\"mini\"\r\n            >\r\n            </el-date-picker>\r\n          </el-form-item>\r\n        </el-form>\r\n\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"add('rulegoto')\">确 定</el-button>\r\n        </div>\r\n      </el-dialog>\r\n      <!-- 编辑跟单 -->\r\n      <el-dialog\r\n        title=\"编辑咨询方式\"\r\n        :visible.sync=\"dialogForm\"\r\n        width=\"30%\"\r\n        :append-to-body=\"true\"\r\n      >\r\n        <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\">\r\n          <el-form-item\r\n            label=\"跟单内容\"\r\n            :label-width=\"formLabelWidth\"\r\n            prop=\"content\"\r\n          >\r\n            <el-input\r\n              type=\"textarea\"\r\n              v-model=\"ruleForm.content\"\r\n            ></el-input>\r\n          </el-form-item>\r\n          <el-form-item\r\n            label=\"跟单终止\"\r\n            :label-width=\"formLabelWidth\"\r\n            prop=\"termination\"\r\n          >\r\n            <el-select v-model=\"ruleForm.termination\" placeholder=\"请选择\">\r\n              <el-option\r\n                v-for=\"item in options\"\r\n                :key=\"item.id\"\r\n                :label=\"item.name\"\r\n                :value=\"item.id\"\r\n              >\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item\r\n            label=\"下次跟单时间\"\r\n            :label-width=\"formLabelWidth\"\r\n            prop=\"lastAt\"\r\n          >\r\n            <el-date-picker\r\n              v-model=\"ruleForm.lastAt\"\r\n              type=\"datetime\"\r\n              placeholder=\"选择日期时间\"\r\n              size=\"mini\"\r\n            >\r\n            </el-date-picker>\r\n          </el-form-item>\r\n          <el-form-item\r\n            label=\"终止时间\"\r\n            :label-width=\"formLabelWidth\"\r\n            prop=\"terminationAt\"\r\n          >\r\n            <el-date-picker\r\n              v-model=\"ruleForm.terminationAt\"\r\n              type=\"datetime\"\r\n              placeholder=\"选择日期时间\"\r\n              size=\"mini\"\r\n            >\r\n            </el-date-picker>\r\n          </el-form-item>\r\n        </el-form>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogForm = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"affirm('ruleForm')\"\r\n            >确 定</el-button\r\n          >\r\n        </div>\r\n      </el-dialog>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { createNamespacedHelpers } from \"vuex\";\r\nconst userModule = createNamespacedHelpers(\"documentary\");\r\nconst { mapState: userState, mapActions: userActions } = userModule;\r\nexport default {\r\n  name: \"\",\r\n  props: {\r\n    ids: {\r\n      type: Number,\r\n    },\r\n  },\r\n  components: {},\r\n  data() {\r\n    return {\r\n      currentPage4: 1, //当前页数\r\n      pagesize: 5, //一页多少条\r\n      pagenum: 1, //当前页数\r\n      query: \"\",\r\n      dialogFormVisible: false,\r\n      dialogForm: false,\r\n      id: \"\",\r\n      options: [\r\n        {\r\n          id: 0,\r\n          name: \"否\",\r\n        },\r\n        {\r\n          id: 1,\r\n          name: \"是\",\r\n        },\r\n      ],\r\n      rulegoto: {\r\n        content: \"\",\r\n        termination: \"\",\r\n        lastAt: \"\",\r\n        terminationAt: \"\",\r\n      },\r\n      ruleForm: {\r\n         content: \"\",\r\n        termination: \"\",\r\n        lastAt: \"\",\r\n        terminationAt: \"\",\r\n      },\r\n      pickerOptions1: {\r\n        shortcuts: [\r\n          {\r\n            text: \"今天\",\r\n            onClick(picker) {\r\n              picker.$emit(\"pick\", new Date());\r\n            },\r\n          },\r\n          {\r\n            text: \"昨天\",\r\n            onClick(picker) {\r\n              const date = new Date();\r\n              date.setTime(date.getTime() - 3600 * 1000 * 24);\r\n              picker.$emit(\"pick\", date);\r\n            },\r\n          },\r\n          {\r\n            text: \"一周前\",\r\n            onClick(picker) {\r\n              const date = new Date();\r\n              date.setTime(date.getTime() - 3600 * 1000 * 24 * 7);\r\n              picker.$emit(\"pick\", date);\r\n            },\r\n          },\r\n        ],\r\n      },\r\n      rules: {\r\n    //     content: [\r\n    //       {\r\n    //         required: true,\r\n    //         message: \"跟单内容不能为空\",\r\n    //         trigger: \"blur\",\r\n    //       },\r\n    //     ],\r\n    //     termination: [\r\n    //       {\r\n    //         required: true,\r\n    //         message: \"跟单内容不能为空\",\r\n    //         trigger: \"blur\",\r\n    //       },\r\n    //     ],\r\n    //     lastAt: [\r\n    //       {\r\n    //         required: true,\r\n    //         message: \"跟单内容不能为空\",\r\n    //         trigger: \"blur\",\r\n    //       },\r\n    //     ],\r\n      },\r\n      formLabelWidth: \"110px\",\r\n    };\r\n  },\r\n  methods: {\r\n    ...userActions([\"getmerchandis\", \"addmerchandis\",\"detelemerchandis\",\"updatemerchandis\"]),\r\n    //编辑\r\n    handleEdit(index, row) {\r\n         this.dialogForm = true;\r\n      this.id = row.id;\r\n      this.ruleForm.content = row.content;\r\n      this.ruleForm.termination = row.termination;\r\n      this.ruleForm.lastAt = row.lastAt;\r\n      this.ruleForm.terminationAt = row.terminationAt;\r\n    },\r\n    //确认编辑\r\n    affirm(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.dialogForm = false;\r\n          this.updatemerchandis({\r\n            id: this.id,\r\n            cid: this.ids,\r\n            content: this.ruleForm.content,\r\n            termination: this.ruleForm.termination,\r\n            lastAt: this.ruleForm.lastAt,\r\n            terminationAt: this.ruleForm.terminationAt,\r\n            pageNo: this.pagenum,\r\n            pageSize: this.pagesize,\r\n          });\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    //删除\r\n    handleDelete(index, row) {\r\n      this.$confirm(\"此操作将永久删除该文件, 是否继续?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n        .then(() => {\r\n          this.detelemerchandis({\r\n            id: row.id,\r\n            cid:this.ids,\r\n            pageNo: this.pagenum,\r\n            pageSize: this.pagesize,\r\n          });\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"已取消删除\",\r\n          });\r\n        });\r\n    },\r\n    // 添加\r\n    add(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.dialogFormVisible = false;\r\n          this.addmerchandis({\r\n            cid: this.ids,\r\n            content: this.rulegoto.content,\r\n            termination: this.rulegoto.termination,\r\n            lastAt: this.rulegoto.lastAt,\r\n            terminationAt: this.rulegoto.terminationAt,\r\n            pageNo: this.pagenum,\r\n            pageSize: this.pagesize,\r\n          });\r\n          this.rulegoto = {}\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    //分页每页多少条\r\n    handleSizeChange(val) {\r\n      this.pagesize = val;\r\n      this.getmerchandis({\r\n           cid: this.ids,\r\n        pageNo: this.pagenum,\r\n        pageSize: this.pagesize,\r\n      });\r\n    },\r\n    //分页当前是第几页\r\n    handleCurrentChange(val) {\r\n      this.pagenum = val;\r\n      this.getmerchandis({\r\n           cid: this.ids,\r\n        pageNo: this.pagenum,\r\n        pageSize: this.pagesize,\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getmerchandis({\r\n      cid: this.ids,\r\n      pageNo: this.pagenum,\r\n      pageSize: this.pagesize,\r\n    });\r\n  },\r\n  watch: {},\r\n  computed: {\r\n    ...userState([\"merchandislist\", \"totalRowg\"]),\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped lang='scss'>\r\n</style>"],"sourceRoot":"src/components/custdetails"}]}